@model IEnumerable<EntityLayer.Concrete.About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif Yap</th>
        <th>Pasif Yap</th>

    </tr>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AboutID</td>
                <td>@item.AboutDetails</td>
                <td><a href="" class="btn btn-warning">Aktif yap</a></td>
                <td><a href="" class="btn btn-danger">Pasif yap</a></td>

            </tr>
        }

    </tbody>
</table>
@Html.Action("AboutPartial", "About")
